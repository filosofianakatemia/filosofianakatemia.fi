{
  "name": "filosofianakatemia-site",
  "version": "0.0.1",
  "description": "Filosofian Akatemia Site",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "etag": "^1.7.0",
    "extendedmind-siteutils": "1.0.11",
    "fs-promise": "^0.3.1",
    "jsdom": "^6.5.1",
    "koa": "^2.0.0-alpha.3",
    "koa-convert": "^1.0.0",
    "koa-logger": "^2.0.0",
    "koa-route": "^3.0.0",
    "koa-static-folder": "^0.1.5",
    "markdown-it": "^4.4.0",
    "markdown-it-container": "^1.0.0",
    "methods": "~1.1.0",
    "path": "^0.11.14",
    "path-to-regexp": "^1.2.0",
    "superagent": "^1.4.0",
    "superagent-promise": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-syntax-async-generators": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "less": "^2.7.0",
    "less-plugin-clean-css": "^1.5.1",
    "protractor": "^2.2.0"
  },
  "scripts": {
    "transpile:modules": "babel --presets stage-0 --plugins syntax-async-functions,syntax-async-generators node_modules/extendedmind-siteutils --out-dir node_modules/extendedmind-siteutils/transpile",
    "transpile:sources": "babel --presets stage-0 --plugins syntax-async-functions,syntax-async-generators es7 --out-dir js",
    "transpile": "npm run transpile:modules && npm run transpile:sources",
    "build:less": "lessc --strict-imports --verbose --clean-css=\"--s1\" static/less/site.less static/css/site.min.css",
    "build": "npm run transpile && npm run build:less",
    "test": "protractor test/protractor.config.js",
    "postinstall": "./node_modules/protractor/bin/webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:filosofianakatemia/filosofianakatemia.fi.git"
  },
  "keywords": [
    "productivity"
  ],
  "author": "Timo Tiuraniemi",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/filosofianakatemia/filosofianakatemia.fi/issues"
  },
  "homepage": "https://filosofianakatemia.fi"
}
