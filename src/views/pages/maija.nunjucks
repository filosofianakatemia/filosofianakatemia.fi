{% extends "ihminen.nunjucks" %}
{% set personName = 'Maija Tiitinen' %}
{% block title %}{{ personName }} | {{ super() }}{% endblock %}
{% block ogtitle %}{{ personName }}{% endblock %}
{% block ogdescription %}Filosofian Akatemian {{ personName }} esitt&auml;ytyy.{% endblock %}
{% block ogurl %}{{ super() }}/ihmiset/maija{% endblock %}
{% block ogimages %}
<meta property="og:image:url" content="https://filosofianakatemia.fi/static/{{ version }}/img/maija-small.jpg" />
<meta property="og:image:secure_url" content="https://filosofianakatemia.fi/static/{{ version }}/img/maija-small.jpg" />
{% endblock %}
{% block description %}Filosofian Akatemian {{ personName }} esitt&auml;ytyy.{% endblock %}
{% block personquote %}Maija on sitä ihmistyyppiä, joka tarttuu tuumasta toimeen ja saa asiat tapahtumaan. Hänessä yhdistyvät harvinaisella tavalla kyky keskustella ja tarkastella asioita monesta näkökulmasta sekä insinöörimäinen taito löytää ongelmiin ratkaisut. Hersyvällä naurulla varustettu tehopakkaus!{% endblock %}
{% block personquoteauthor %}Tytti Kokko{% endblock %}
